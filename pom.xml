          <artifactId>org.eclipse.core.runtime</artifactId>
          <version>3.12.0.v20160606-1342</version>
          <scope>system</scope>
          <systemPath>${pom.basedir}/lib/org.eclipse.core.runtime_3.12.0.v20160606-1342.jar</systemPath>
