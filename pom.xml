      <dependency>
          <groupId>org.eclipse.jface.text</groupId>
          <artifactId>org.eclipse.jface.text</artifactId>
          <version>3.11.2.v20170220-1911</version>
          <scope>system</scope>
          <systemPath>${pom.basedir}/lib/org.eclipse.jface.text_3.11.2.v20170220-1911.jar</systemPath>
